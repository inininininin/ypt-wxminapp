button::after{
  border: none;
}

.pageLoading{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  opacity: 1;
  background: #e5e5e5;
  /* background: rgba(0,0,0,0.5); */
  z-index: 3;
}
.pageLoading>image{
  width: 100rpx;
  height: 100rpx;
  margin: 450rpx 325rpx;
}
.showNone{
  width: 100%;
  font-size: 32rpx;
  line-height: 300rpx;
  text-align: center;
}

.linef2f2f2{
