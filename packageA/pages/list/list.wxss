/* packageA/pages/list/list.wxss */
.userRowsItem{
  margin: 0 auto;
  width: calc(100% - 72rpx);
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  /* padding: 0 36rpx; */
  box-sizing: border-box;
  line-height: 100rpx;
  border-bottom: 1rpx solid #e5e5e5;
  min-height: 100rpx;
}
 .success{
  color: #10da3c;
}
 .error{
  color: #e40606;
}