page {
  height: 100%;
  background: #f2f2f2;
}

.container {
  box-sizing: border-box;
  position: relative;
  margin-bottom: 24rpx;
}

.header {
  position: fixed;
  top: 0;
  width: 100%;
  background: #fff;
  z-index: 2;
}

.title-bar {
  display: flex;
  align-items: center;
}

.title-bar>view {
  height: 60rpx;
  width: 460rpx;
  background: #fff;
  border-radius: 30rpx;
  float: left;
  line-height: 60rpx;
  vertical-align: middle;
  margin-left: 10rpx;
}

.back {
  width: 18rpx;
  height: 30rpx;
  display: inline-block;
  padding: 20rpx;
  margin-left: 12rpx;
}
.evahospital {
  width: 610rpx;
  text-align: center;
  opacity: 1;
  font-size: 36rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  letter-spacing: 0rpx;
}

.firstTimeIncome {
  width: 100%;
  margin-top: 24rpx;
  background: #fff;
  /* padding-bottom: 100rpx; */
}

.clear {
  clear: both;
}

.incomeTitle {
  border-left: 9rpx solid rgb(69, 154, 233);
  height: 24rpx;
  padding-left: 12rpx;
  line-height: 24rpx;
  display: inline-block;
  margin: 34rpx 0 10rpx 32rpx;
}

.firstIncomeItem {
  display: block;
  height: 132rpx;
  border-bottom: 1rpx solid #e5e5e5;
  width: 718rpx;
  margin-left: 32rpx;
}

.firstIncomeItem>view {
  display: inline-block;
  padding-top: 30rpx;
}

.firstIncomeItem>view view:first-child {
  font-size: 32rpx;
  color: rgb(51, 51, 51);
  line-height: 32rpx;
  /* padding-top: 30rpx; *//* float: left; */
  margin: 0 0 16rpx;
}

.firstIncomeItem>view view:first-child text {
  color: rgb(255, 26, 46);
  margin-left: 36rpx;
  font-size: 26rpx;
}

.firstIncomeItem>view view:nth-child(2) {
  font-size: 24rpx;
  color: rgb(102, 102, 102);
  /* float: left; */
}

.firstIncomeItem .finish {
  float: right;
  display: inline-block;
  width: 160rpx;
  height: 60rpx;
  margin-right: 32rpx;
  border-radius: 30rpx;
  border: none;
  background: rgb(229, 229, 229);
  color: rgb(102, 102, 102);
  text-align: center;
  line-height: 60rpx;
  margin-top: 36rpx;
  font-size: 26rpx;
}

.firstIncomeItem .nofinish {
  float: right;
  display: inline-block;
  width: 160rpx;
  height: 60rpx;
  margin-right: 32rpx;
  border-radius: 30rpx;
  border: 1rpx solid rgb(226, 64, 63);
  color: rgb(226, 64, 63);
  text-align: center;
  line-height: 60rpx;
  margin-top: 36rpx;
  font-size: 26rpx;
}

.incomeintro {
  margin-left: 0 !Important;
  color: rgb(153, 153, 153) !important;
}

.progressBar {
  margin-top: 30rpx;
  height: 26rpx;
}

.progressBar>view {
  width: 140rpx;
  height: 10rpx;
  border-radius: 10rpx;
  background: rgb(229, 229, 229);
  overflow: hidden;
  float: left;
  /* display: inline-block; */
}

.progressBar>view text {
  display: inline-block;
  background: rgb(255, 26, 46);
  height: 10rpx;
  /* width: 20%; */
  margin-left: 0 !important;
  float: left;
}

.progressBar>text {
  display: inline-block;
  font-size: 26rpx;
  line-height: 26rpx;
  float: left;
  margin-left: 32rpx;
  margin-top: -13rpx;
  color: rgb(153, 153, 153);
}

.popwindow {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  z-index: 999;
  background: rgba(0, 0, 0, 0.5);
}

.popwindow>view {
  width: 540rpx;
  height: 948rpx;
  background: #fff;
  margin: 170rpx auto;
  position: relative;
}

.poptitle {
  height: 86rpx;
  width: 100%;
  position: relative;
  text-align: center;
  font-size: 28rpx;
  color: rgb(51, 51, 51);
  line-height: 86rpx;
  border-bottom: 1rpx solid #e5e5e5;
}

.delete {
  width: 20rpx;
  height: 20rpx;
  float: right;
  margin: 33rpx 32rpx;
}

.popindex {
  font-size: 26rpx;
  color: rgb(51, 51, 51);
}

.popindex text {
  display: block;
  margin: 20rpx 32rpx;
  line-height: 37rpx;
}

.popindex image {
  width: 176rpx;
  height: 174rpx;
  right: -50rpx;
  position: absolute;
  bottom: 0;
}
.shareFont{
  width: 260rpx;
  height: 80rpx;
  border-radius: 40rpx;
  position: fixed;
  right: 30rpx;
  bottom: 30rpx;
  z-index: 1000;
  background: #fff;
  text-align: center;
  font-size: 32rpx;
  line-height:80rpx; 
  color: #2B77EF;
  box-shadow: 2rpx 4rpx 12rpx #e5e5e5;
}
.share{
  width: 38rpx;
  height: 38rpx;
  float: left;
  margin-top: -8rpx;
margin-left: 45rpx;
}
.font{
  width: 40rpx;
  height: 34rpx;
   float: left;
  margin-top: -8rpx;
margin-left: 45rpx;
}
.shareFont>view{
  text-align: center;
  display: inline-block;
  height: 22rpx;
  width: 128rpx;
  border-right: 4rpx solid #e5e5e5;
  margin-top: 29rpx;
}
.shareFont>view:last-child{
  border-right: 0;
}